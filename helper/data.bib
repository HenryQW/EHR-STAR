  keywords = {applications,coordinated multiple-views,epinome,infectious disease outbreak,information visualization,simulations,user study,visual analytics},
  month    = {mar},
  number   = {2},
  pages    = {89--95},
  title    = {{Epinome: A Visual-Analytics Workbench for Epidemiology Data}},
  volume   = {32},
