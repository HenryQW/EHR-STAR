  keywords  = {Geospatial multivariate visualization,Graphical user interface,Healthcare data,Information visualization},
  number    = {2},
  pages     = {223--234},
  publisher = {Elsevier Inc.},
  title     = {{Community Health Map: A geospatial and multivariate data visualization tool for public health datasets}},
  volume    = {29},
  year      = {2012}
}
